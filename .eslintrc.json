{
  "plugins": ["babel", "react"],
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "object-curly-newline": 0,
    "function-paren-newline": 0,
    "camelcase": 1,
    "arrow-parens": 0
  }
}
